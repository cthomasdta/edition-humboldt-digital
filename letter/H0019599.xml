                    <pb n="2r"/>
                    <gap reason="empty" unit="pages" quantity="1"/>
                    <pb n="2v"/>
                    <address>
                        <addrLine>
                            <choice>
                                <abbr>Sr</abbr>
                                <expan>Seiner</expan>
                            </choice> Wohlgeboren</addrLine>
                        <addrLine>Herrn <persName ref="http://d-nb.info/gnd/116911921">Otto
                            Schomburgk</persName>
                        </addrLine>
                        <addrLine>mit einem Buche</addrLine>
                        <addrLine>
                            <bibl corresp="https://edition-humboldt.de/v8/6NN5493H">le Sahara</bibl>
                        </addrLine>
                        <addrLine>AlHumboldt</addrLine>
                    </address>
                </closer>
